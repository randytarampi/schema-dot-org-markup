{
  "name": "@randy.tarampi/schema-dot-org-types",
  "version": "0.2.0-2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Classes with flow types to encapsulate schema.org markup attributes.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./lib && npx babel src -d lib && npx babel tools -d bin && rollup -c -i src/index.js && flow-copy-source src lib",
    "prepare": "npm run build",
    "test": "npx mocha --compilers js:babel-register ./test",
    "transform-schema": "node bin/transform-schema.js"
  },
  "files": [
    "lib",
    "index.js",
    "index.module.js"
  ],
  "module": "index.module.js",
  "keywords": [
    "schema.org",
    "jsonld"
  ],
  "author": "Zillow (https://github.com/zillow)",
  "repository": "https://github.com/randytarampi/schema-dot-org-markup",
  "license": "ISC",
  "devDependencies": {
    "JSONStream": "^1.3.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-register": "^6.26.0",
    "commander": "^2.9.0",
    "decamelize": "^1.2.0",
    "event-stream": "^3.3.4",
    "find-up": "latest",
    "flow-copy-source": "^1.1.0",
    "load-json-file": "latest",
    "mocha": "^4.0.1",
    "request": "^2.81.0",
    "rollup": "^0.66.0",
    "schema.org": "^3.1.0",
    "url-template": "^2.0.8"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "object-assign": "^4.1.1"
  },
  "engines": {
    "npm": ">=5.2"
  }
}
